{
  "expo": {
    "name": "SpeakFlow AI",
    "slug": "voiceflow",
    "owner": "andreaskalkusinski",
    "version": "2.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
